{
  "name": "default-vanilla",
  "version": "0.0.1",
  "description": "Boilerplate vanilla-ish JS",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && npm run watch",
    "dist": "npm run js-dist && npm run css-dist && npm run inline-dist",
    "watch": "node ./node_modules/watch/cli.js 'npm run build' src -d -u",
    "build": "npm run js-dev && npm run css && npm run inline",
    "js-dev": "browserify -d -v -t babelify src/js/app.js -o dist/js/app.js",
    "js-dist": "browserify -v -t babelify -t uglifyify src/js/app.js -o dist/js/app.build.js && npm run js-min",
    "js-min": "uglifyjs dist/js/app.build.js --compress unsafe,keep_fargs,drop_console,collapse_vars,if_return,join_vars,cascade,booleans,loops,unused,comparisons,conditionals,drop_debugger,dead_code,properties,sequences --screw-ie8 --mangle --comments --source-map dist/js/app.min.js.map --output dist/js/app.min.js",
    "css": "postcss --use postcss-import --use autoprefixer src/css/styles.css --out dist/css/styles.css",
    "css-dist": "npm run css && npm run css-min",
    "css-min": "purifycss dist/css/styles.css src/index.html dist/js/app.min.js --out | cssnano > dist/css/styles.min.css",
    "inline": "critical src/index.html --base dist/ -H dist/index.html",
    "inline-dist": "critical src/index.html --minify --extract --base dist/ -H dist/index.html",
    "watch-old": "npm run js-watch && npm run css",
    "watch-dist-old": "npm run js-watch-dist && npm run css-watch",
    "js-old-rollup": "rollup -c",
    "js-watch-old": "watchify -v -d -t babelify -e src/js/app.js -o `echo > dist/js/app.js && npm run inline`",
    "js-watch-dist": "watchify -v -t babelify -t uglifyify -e src/js/app.js -o `npm run js-min`",
    "css-watch-old": "postcss --watch --use postcss-import --use autoprefixer src/css/styles.css -o dist/css/styles.css"
  },
  "keywords": [],
  "author": "Deryck Henson",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "critical": "^0.7.3",
    "cssnano": "^3.7.0",
    "cssnano-cli": "^1.0.4",
    "postcss": "^5.0.21",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2",
    "purify-css": "^1.1.9",
    "request": "^2.72.0",
    "reqwest": "^2.0.5",
    "rollup": "^0.29.0",
    "rollup-plugin-auto-transform": "^1.0.1",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-commonjs": "^3.0.0",
    "rollup-plugin-node-resolve": "^1.7.0",
    "rollup-plugin-uglify": "^0.3.1",
    "socket.io": "^1.4.6",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1",
    "watch": "^0.18.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "fastdom": "^1.0.2",
    "immutable": "^3.8.1",
    "lodash": "^4.13.1"
  }
}
